{
  "buildCommand": "cd frontend && npm install && ../../frontend/node_modules/vite/bin/vite.js build",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {"source": "/(.*)", "destination": "/"}
  ]
}